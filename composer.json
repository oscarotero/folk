        "oscarotero/inline-svg": "^2.0",
        "form-manager/form-manager": "^5.0",
        "gettext/gettext": "^5.0.0",
		"gettext/robo": "^4.0.0",
		"gettext/php-scanner": "^1.0.0",
		"gettext/js-scanner": "^1.0.0",
		"gettext/translator": "^1.0.0",
		"gettext/json": "^1.0.0",
        "imagecow/imagecow": "^2.1",
        "psr/http-server-middleware": "^1.0",
        "middlewares/utils": "^3.0",
