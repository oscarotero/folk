        "middlewares/filesystem": "^2.0",
        "middlewares/method-override": "^2.0",
        "middlewares/negotiation": "^2.0",
        "middlewares/request-handler": "^2.0",
        "middlewares/trailing-slash": "^2.0",
        "middlewares/base-path": "^2.0"
